{
  "name": "@casper124578/utils",
  "version": "0.1.0-alpha",
  "description": "A package with useful node util functions",
  "main": "index.js",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/esm/index.js"
    },
    "./": "./"
  },
  "scripts": {
    "build": "rimraf ./dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project configs/tsconfig.esm.json",
    "build:cjs": "tsc --project configs/tsconfig.cjs.json",
    "format:quick": "pretty-quick --staged --pattern \"**/*.{js,jsx,ts,tsx,md,css,json}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "prepare": "husky install"
  },
  "keywords": [
    "utils",
    "util functions"
  ],
  "author": {
    "name": "Dev-CasperTheGhost",
    "url": "https://caspertheghost.me"
  },
  "dependencies": {
    "yup": "0.32.9"
  },
  "peerDependencies": {
    "yup": "0.32.9"
  },
  "peerDependenciesMeta": {
    "yup": {
      "optional": true
    }
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^0.0.14",
    "esbuild": "^0.12.9",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  }
}
