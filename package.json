      "optional": true
    }
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^0.0.14",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
